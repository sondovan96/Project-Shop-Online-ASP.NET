@model IEnumerable<ProjectShopASP.Models.MENU>
<div class="header_top">
    <div class="row align-items-center">
        <div class="col-lg-6 col-md-6">
            <div class="switcher">
                <ul>
                    <li class="languages">
                        <a href="#"><img src="\Assets\client\img\logo\fontlogo.jpg" alt=""> English <i class="fa fa-angle-down"></i></a>
                        <ul class="dropdown_languages">
                            <li><a href="#"><img src="\Assets\client\img\logo\fontlogo.jpg" alt=""> English</a></li>
                            <li><a href="#"><img src="\Assets\client\img\logo\fontlogo2.jpg" alt=""> Vietnamese </a></li>
                        </ul>
                    </li>

                    <li class="currency">
                        <a href="#"> Currency : $ <i class="fa fa-angle-down"></i></a>
                        <ul class="dropdown_currency">
                            <li><a href="#"> Dollar (USD)</a></li>
                            <li><a href="#"> Euro (EUR)  </a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <div class="col-lg-6 col-md-6">
            <div class="header_links">
                <ul>
                    @foreach (var item in Model)
                    {
                        <li>
                            <a href="@item.link" target="@item.target">@item.text</a>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>